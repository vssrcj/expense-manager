{
   "extends": "airbnb",
   "plugins": [
       "react",
       "jsx-a11y",
       "import"
   ],
   "parser": "babel-eslint",
   "settings": {
     "import/resolver": {
       "node": {
         "paths": ["src"]
       }
     }
   },
   "globals": {
      "test": true,
      "describe": true,
      "beforeAll": true,
      "afterAll": true,
      "expect": true
   },
   "rules": {
     "import/no-extraneous-dependencies": 0,
     "import/no-unresolved": 0,
     "import/extensions": 0,
     "import/prefer-default-export": 0,
     "react/jsx-filename-extension": 0,
     "react/jsx-indent": [2, 3],
     "react/jsx-indent-props": [2, 3],
     "indent": [2, 3, { "SwitchCase": 1 }],
     "react/require-default-props": 0,
     "react/no-unescaped-entities": 0,
     "global-require": 0,
     "linebreak-style": 0,
     "no-unused-vars": 1,
     "func-names": 0,
     "jsx-a11y/anchor-is-valid": 0
   }
}